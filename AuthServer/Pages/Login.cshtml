@page "/Login"
@model AuthServer.Pages.LoginModel

<h2>AuthServer Login</h2>

<form method="post">
    <div>
        <label>Username</label>
        <input asp-for="Username" />
    </div>

    <div>
        <label>Password</label>
        <input asp-for="Password" type="password" />
    </div>

    <button type="submit">Login</button>
</form>
